generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL") //conecta com o banco de dados atrav√©s do .env
}

// Gera as Models (Tabelas) do banco de dados
model Transportadora {  
  id    String @id @default(auto()) @map("_id") @db.ObjectId 
  transportadora String?
  motorista String?
  rg_motorista String?
  ajudante String
  rg_ajudante String
  placa String?
  dth_entrada String?
  dth_saida String
  empresa String
}