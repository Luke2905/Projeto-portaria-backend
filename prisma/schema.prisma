generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL") //conecta com o banco de dados atrav√©s do .env
}

// Gera as Models (Tabelas) do banco de dados
model Transportadora {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  ajudante       String
  dth_entrada    String
  dth_saida      String
  empresa        String
  motorista      String
  placa          String
  rg_ajudante    String
  rg_motorista   String
  transportadora String
}

// Gera a Table de Usuarios
model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  acessos  String
  email    String
  name     String
  password String
  user     String
}
