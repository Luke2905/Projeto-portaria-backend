generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Transportadora {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  transportadora String ?
  motorista      String ?
  rg_motorista   String ?
  ajudante       String 
  rg_ajudante    String
  placa          String ?
  dth_entrada    String ?
  dth_saida      String
  empresa        String 
  tipo           String ?
  observacao     String
}

model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  acessos  String
  email    String ? @unique
  name     String ?
  password String ?
  user     String ?
}
